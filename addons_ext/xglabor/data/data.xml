<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for xglabor.labor -->
        <record id="seq_xglabor_labor_number" model="ir.sequence">
            <field name="name">Labor Number</field>
            <field name="code">xglabor.labor.number</field>
            <field name="prefix">LL-LB-</field>
            <field name="padding">6</field>
        </record>

        <!-- Sequences for xglabor.workorder -->
        <record id="seq_xglabor_workorder_number" model="ir.sequence">
            <field name="name">WorkOrder Number</field>
            <field name="code">xglabor.workorder.number</field>
            <field name="prefix">LL-WO-</field>
            <field name="padding">7</field>
        </record>


        <record id="data_xglabor_type_0" model="xglabor.type">
            <field name="name">丝绳工</field>
        </record>

        <record id="data_xglabor_type_1" model="xglabor.type">
            <field name="name">搬运工</field>
        </record>

        <record id="data_xglabor_type_2" model="xglabor.type">
            <field name="name">安全员</field>
        </record>

        <record id="data_xglabor_type_3" model="xglabor.type">
            <field name="name">叉车工</field>
        </record>

        <record id="data_xglabor_type_4" model="xglabor.type">
            <field name="name">磅工</field>
        </record>

    </data>


</odoo>